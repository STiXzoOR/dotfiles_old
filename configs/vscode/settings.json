{
    "workbench.iconTheme": "vscode-icons",
    "javascript.validate.enable": false,
    "editor.formatOnSave": true,
    "editor.fontSize": 15,
    "editor.fontFamily": "'DejaVu Sans Mono',Menlo, Monaco, 'Courier New', monospace",
    "emmet.triggerExpansionOnTab": true,
    "editor.quickSuggestionsDelay": 750,
    "window.openFilesInNewWindow": "on",
    "window.openFoldersInNewWindow": "on",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.editor.enablePreview": false,
    "editor.wordWrap": "on",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "vue-html": "html"
    },
    "editor.showFoldingControls": "always",
    "workbench.colorTheme": "Cobalt2",
    "git.postCommitCommand": "push",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.integrated.fontSize": 14,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "workbench.colorCustomizations": {
        "terminal.background": "#173347",
        "terminal.foreground": "#feffff",
        "terminalCursor.foreground": "#f4d300",
        "terminal.ansiBlack": "#173347",
        "terminal.ansiBlue": "#1477db",
        "terminal.ansiCyan": "#00c5c7",
        "terminal.ansiGreen": "#3cdf2b",
        "terminal.ansiMagenta": "#ff2b70",
        "terminal.ansiRed": "#ff2600",
        "terminal.ansiWhite": "#c7c7c7",
        "terminal.ansiYellow": "#ffe700",
        "terminal.ansiBrightBlack": "#676767",
        "terminal.ansiBrightBlue": "#6871ff",
        "terminal.ansiBrightCyan": "#79e7fa",
        "terminal.ansiBrightGreen": "#42d425",
        "terminal.ansiBrightMagenta": "#ff76ff",
        "terminal.ansiBrightRed": "#f9291b",
        "terminal.ansiBrightWhite": "#feffff",
        "terminal.ansiBrightYellow": "#f1d000",
        "terminal.selectionBackground": "#3e82af93"
    },
    "workbench.sideBar.location": "right",
    "window.zoomLevel": 0
}